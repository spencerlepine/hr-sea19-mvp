{
  "name": "hr-sea19-mvp",
  "version": "1.0.0",
  "description": "Fullstack MVP project from Hack Reactor",
  "main": "server/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencerlepine/hr-sea19-mvp.git"
  },
  "keywords": [],
  "author": "Spencer Lepine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spencerlepine/hr-sea19-mvp/issues"
  },
  "homepage": "https://github.com/spencerlepine/hr-sea19-mvp#readme",
  "scripts": {
    "start": "node server/app.js",
    "server:dev": "nodemon server/app.js",
    "test": "jest",
    "test:watch": "jest --onlyChanged --watch",
    "lint:check": "eslint . --ext .js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2015": true
    },
    "ignorePatterns": [],
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-undef": "off",
      "no-unused-vars": "off"
    }
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.13"
  }
}